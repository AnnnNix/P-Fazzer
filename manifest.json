{
  "manifest_version": 2,
  "name": "P-FazzER - Sensitive File Checker",
  "version": "1.2",
  "description": "Scan websites for exposed sensitive files.",
  "permissions": [
    "tabs",
    "webNavigation",
    "<all_urls>",
    "storage",
    "downloads"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Check History",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}
